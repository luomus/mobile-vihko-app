{
  "expo": {
    "name": "Mobiilivihko",
    "slug": "mobile-vihko-app",
    "privacy": "public",
    "owner": "luomus",
    "platforms": [
      "android",
      "ios"
    ],
    "version": "1.3.26",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.luomus.mobi",
      "buildNumber": "1.0.7",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationWhenInUseUsageDescription": "Mobiilivihko requires permissions to track the route while the app is on the background."
      }
    },
    "android": {
      "package": "org.luomus.mobi",
      "versionCode": 53,
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW"
      ],
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#2691D9",
      "translucent": false
    },
    "extra": {
      "eas": {
        "projectId": "334626ac-7ade-4e01-988c-428696a37913"
      }
    },
    "hooks": {
      "postPublish": []
    },
    "plugins": [
      "sentry-expo"
    ]
  }
}
