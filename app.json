{
  "expo": {
    "name": "Mobiilivihko",
    "slug": "mobile-vihko-app",
    "privacy": "public",
    "owner": "luomus",
    "platforms": [
      "android",
      "ios"
    ],
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.luomus.mobi",
      "buildNumber": "1.0.2",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "org.luomus.mobi",
      "versionCode": 50,
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#2691D9",
      "translucent": false
    },
    "extra": {
      "eas": {
        "projectId": "334626ac-7ade-4e01-988c-428696a37913"
      }
    },
    "hooks": {
      "postPublish": [{
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "luomus",
            "project": "mobiilivihko",
            "authToken": "SENTRY_AUTH_TOKEN"
          }
      }]
    },
    "plugins": [
      "sentry-expo"
    ]
  }
}